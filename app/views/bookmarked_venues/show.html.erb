<div class="page-header">
  <h3>Bookmarked Venue #<%= @bookmarked_venue.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Dish</dt>
      <dd><%= @bookmarked_venue.dish_id %></dd>

      <dt>Venue</dt>
      <dd><%= @bookmarked_venue.venue_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/bookmarked_venues" class="btn btn-primary">
        Back
      </a>
      <a href="/bookmarked_venues/<%= @bookmarked_venue.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_bookmarked_venue/<%= @bookmarked_venue.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
