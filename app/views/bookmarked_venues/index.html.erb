<div class="page-header">
  <h1>
    Bookmarked venues
    <a href="/bookmarked_venues/new" class="btn btn-lg btn-success">New Bookmarked venue</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Dish</th>
        <th>Venue</th>
        <th>Actions</th>
      </tr>

      <% @bookmarked_venues.each do |bookmarked_venue| %>
      <tr>
        <td><%= bookmarked_venue.dish.dish_names %></td>
        <td><%= bookmarked_venue.venue.venue_name %></td>
        <td>
          <a href="/bookmarked_venues/<%= bookmarked_venue.id %>" class="btn btn-primary">Show</a>
          <a href="/bookmarked_venues/<%= bookmarked_venue.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_bookmarked_venue/<%= bookmarked_venue.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @bookmarked_venues, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
