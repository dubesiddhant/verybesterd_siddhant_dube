<div class="page-header">
  <h1>
    Dish lists
    <a href="/dish_lists/new" class="btn btn-lg btn-success">New Dish list</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Dish names</th>
        <th>Dish desc</th>
        <th>Cuisine</th>
        <th>Actions</th>
      </tr>

      <% @dish_lists.each do |dish_list| %>
      <tr>
        <td><%= dish_list.dish_names %></td>
        <td><%= dish_list.dish_desc %></td>
        <td><%= dish_list.cuisine %></td>
        <td>
          <a href="/dish_lists/<%= dish_list.id %>" class="btn btn-primary">Show</a>
          <a href="/dish_lists/<%= dish_list.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_dish_list/<%= dish_list.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
